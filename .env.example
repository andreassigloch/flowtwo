# GraphEngine Environment Configuration

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_api_key_here

# LLM Configuration
LLM_MODEL=claude-sonnet-4-5-20250929
LLM_MAX_TOKENS=4096
LLM_TEMPERATURE=0.7

# LLM Provider Selection (CR-034)
# Options: 'anthropic' (default) or 'openai' (for LM Studio, OpenRouter, etc.)
LLM_PROVIDER=anthropic

# OpenAI-Compatible Provider Settings (when LLM_PROVIDER=openai)
# For LM Studio (local):
#   OPENAI_BASE_URL=http://192.168.78.202:1234/v1
#   OPENAI_MODEL=devstral-small-2505-mlx
#   OPENAI_API_KEY=not-needed
# For OpenRouter (cloud):
#   OPENAI_BASE_URL=https://openrouter.ai/api/v1
#   OPENAI_MODEL=mistralai/devstral-small:free
#   OPENAI_API_KEY=your_openrouter_key
OPENAI_BASE_URL=http://localhost:1234/v1
OPENAI_MODEL=devstral-small-2505-mlx

# AgentDB Configuration (LLM Agent Shared Memory)
# Backend options: 'agentdb' (persistent), 'memory' (dev), 'disabled' (no caching)
AGENTDB_ENABLED=true
AGENTDB_BACKEND=memory
AGENTDB_URL=/tmp/graphengine-agentdb.db
AGENTDB_CACHE_TTL=1800
AGENTDB_SIMILARITY_THRESHOLD=0.85

# OpenAI Embeddings (for AgentDB vector search with backend=agentdb)
# Falls back to OPENAI_API_KEY or ANTHROPIC_API_KEY if not set
OPENAI_EMBEDDING_API_KEY=
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSION=1536

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password_here

# Import/Export Configuration
IMPORT_EXPORT_DIR=./exports
DEFAULT_EXPORT_FILENAME=system-export.txt
